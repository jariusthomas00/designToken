        "type": "color"
      },
      "padding": {
        "value": "{dimension.md}",
        "type": "dimension"
      }
    },
    "boxShadow": {
      "default": {
        "value": [
          {
            "x": 5,
            "y": 5,
            "spread": 3,
            "color": "rgba({shadows.default}, 0.15)",
            "blur": 5,
            "type": "dropShadow"
          },
          {
            "x": 4,
            "y": 4,
            "spread": 6,
            "color": "#00000033",
            "blur": 5,
            "type": "innerShadow"
          }
        ],
        "type": "boxShadow"
      }
    },
